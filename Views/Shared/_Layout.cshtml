    <script>
        
    function sendMessage()
    {
        var n = $('#contactName').val();
        var s = $('#contactEmail').val();
        var b = $('#contactBody').val();
        
        if (n.trim() === "" || s.trim() === "" || b.trim()==="" )
        {

            swal({
                title: "Stunners FC",
                text: 'Please fill all fields!',
                type: 'warning',
                showConfirmButton: true
            });


        } else {
            $('#msgSubmit').val("Sending...");
            $('#msgSubmit').prop('disabled', true);

            $.ajax({
                url: '@Url.Action("SendEmail", "Home")',
                type: 'GET',
                dataType: 'json',
                data: { name: n, sub: s, body: b },
                success: function (data) {
                    $('#msgSubmit').val("SEND MESSAGE");
                    $('#msgSubmit').prop('disabled', false);

                    if (data == true) {
                        swal({
                            title: "Stunners FC",
                            text: 'Your message has been delivered',
                            type: 'success',
                            timer: 2000,
                            showConfirmButton: false
                        });


                        $('#contactName').val("");
                        $('#contactEmail').val("");
                        $('#contactBody').val("");

                        
                    }
                    else {
                        swal({
                            title: "Stunners FC",
                            text: 'Some issues with your request',
                            type: 'error',
                            timer: 2000,
                            showConfirmButton: false
                        });


                    }

                }
            });

            
        }
        
    }
    </script>

