    <section id="home">
        <div id="home-carousel" class="carousel slide" data-interval="false">
            <ol class="carousel-indicators">
                <li data-target="#home-carousel" data-slide-to="0" class="active"></li>
                <li data-target="#home-carousel" data-slide-to="1"></li>
                <li data-target="#home-carousel" data-slide-to="2"></li>
            </ol>
            @{
                var bg1 = Session["backgroundPath1"].ToString();
                var bg2 = Session["backgroundPath2"].ToString();
                var bg3 = Session["backgroundPath3"].ToString();

            }
            <!--/.carousel-indicators-->
            <div class="carousel-inner">
                <div class="item active" style="background-image: url('@bg1'); ">
                    <div class="carousel-caption">
                        <div class="animated bounceInRight">
                            @{
                                if (Session["nextMatch"] != null)
                                {
                                    Match_Details m = (Match_Details)Session["nextMatch"];
                                    DateTime d = (DateTime)m.match_date;
                                    <h3>Next Match:  <b style="color:white">@d.ToString(new System.Globalization.CultureInfo("en-US"))</b></h3>
                                    <marquee onmouseover="this.stop();"
                                             onmouseout="this.start();"><h2 style="color:white">@m.team_one vs <span style="color:black">@m.team_two</span> </h2></marquee>

                                }
                                else
                                {
                                    <h2>Next Match</h2>
                                    <p>No Match available</p>
                                }

                            }

                        </div>
                    </div>
                </div>
                <div class="item" style="background-image: url('@bg2')">
                    <div class="carousel-caption">
                        <div class="animated bounceInDown">

                            @{
                                if (Session["nextMatch"] != null)
                                {
                                    Match_Details m = (Match_Details)Session["nextMatch"];
                                    DateTime d = (DateTime)m.match_date;
                                    <h3>Next Match:  <b style="color:white">@d.ToString(new System.Globalization.CultureInfo("en-US"))</b></h3>
                                    <marquee onmouseover="this.stop();"
                                             onmouseout="this.start();"><h2 style="color:white">@m.team_one vs @m.team_two</h2></marquee>

                                }
                                else
                                {
                                    <h2>Next Match</h2>
                                    <p>No Match available</p>
                                }

                            }

                        </div>
                    </div>
                </div>
                <div class="item" style="background-image: url('@bg3')">
                    <div class="carousel-caption">
                        <div class="animated bounceInUp">
                            @{
                                if (Session["nextMatch"] != null)
                                {
                                    Match_Details m = (Match_Details)Session["nextMatch"];
                                    DateTime d = (DateTime)m.match_date;
                                    <h3>Next Match:  <b style="color:white">@d.ToString(new System.Globalization.CultureInfo("en-US"))</b></h3>
                                    <marquee onmouseover="this.stop();"
                                             onmouseout="this.start();"><h2 style="color:white">@m.team_one vs @m.team_two</h2></marquee>

                                }
                                else
                                {
                                    <h2>Next Match</h2>
                                    <p>No Match available</p>
                                }

                            }

                        </div>
                    </div>
                </div>
            </div>
            <!--/.carousel-inner-->
            <nav id="nav-arrows" class="nav-arrows hidden-xs hidden-sm visible-md visible-lg">
                <a class="sl-prev hidden-xs" href="#home-carousel" data-slide="prev">
                    <i class="fa fa-angle-left fa-3x"></i>
                </a>
                <a class="sl-next" href="#home-carousel" data-slide="next">
                    <i class="fa fa-angle-right fa-3x"></i>
                </a>
            </nav>
        </div>
    </section>
